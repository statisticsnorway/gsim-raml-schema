#%RAML 1.0 Library
uses:
  Link: ./annotation/Link.raml
  IdentifiableArtefact: ./abstract/IdentifiableArtefact.raml


types:

  ProcessStepInstance:
    description: An executed step in a Business Process. A Process Step Instance specifies the actual inputs to and outputs from for an occurrence of a Process Step.
    displayName: Process step instance
    type: [ IdentifiableArtefact.IdentifiableArtefact]

    properties:
      codeBlock:
        type: string
        displayName: Code block
        description: The action to take place as executable code
    
      codeType:  #TODO: maybe not nessessary
        type: string
        displayName: Type of code
        description: Type of code (language, system executed in, etc)

      parameterInputs?:
        type: string[]
        displayName: Parameter inputs
        (Link.types): [ParameterInput]

      transformableInputs?:
        type: string[]
        displayName: Transformable inputs
        (Link.types): [TransformableInput]

      processSupportInputs?:
        type: string[]
        displayName: Process support inputs
        (Link.types): [ProcessSupportInput]

      transformedOutputs?:
        type: string[]
        displayName: Transformed outputs
        (Link.types): [TransformedOutput]

      processMetrics?:
        type: string[]
        displayName: Process metrics
        (Link.types): [ProcessMetric]

      processExecutionLog?:
        type: string
        displayName: Process execution log
        (Link.types): [ProcessExecutionLog]

    #example: !include ../examples/ProcessStepInstance.json

