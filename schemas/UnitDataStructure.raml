#%RAML 1.0 Library
uses:
  Link: ./annotation/Link.raml
  Doc: ./annotation/Doc.raml
  IdentifiableArtefact: ./abstract/IdentifiableArtefact.raml
  DataStructure: ./abstract/DataStructure.raml

types:
  UnitDataStructure:
    (Doc.objectGsimGroup): STRUCTURE
    (Doc.objectMicroService): CATALOG
    description: Describes the structure of a Unit Data Set.
    displayName: Unit data structure
    type: [ IdentifiableArtefact.IdentifiableArtefact, DataStructure.DataStructure ]

    properties:
      logicalRecords:
        type: string[]
        displayName: Logical records
        (Link.types): [LogicalRecord]

    example: !include ../examples/_main/UnitDataStructure_Family_1.json
